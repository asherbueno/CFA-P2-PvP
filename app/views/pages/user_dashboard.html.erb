
<body class="post-index">
<h3> Dashboard </h3>

      <% current_user.posts.each do |post| %>
      <div class="card">
        <div class="card-image">
          <%= image_tag post.image.url(:medium) %>
      <h6><%= post.name %></h6>
      <p>$<%= post.price %></p>
      <p><%= link_to 'Edit', edit_post_path(post) %>  |
      <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></p>
      <%end%>
    </body>
